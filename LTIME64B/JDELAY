import java.util.Scanner;

class ltsept18 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t=sc.nextInt();
		int iterate=0;
		while(iterate++<t) {
			int n=sc.nextInt();
			int ans=0;
			for(int i=0;i<n;i++) {
				int a=sc.nextInt();
				int b=sc.nextInt();
				if(b-a>5)
					ans++;
			}
			System.out.println(ans); 
		}

	}

}
